{
    "extends": ["prettier"],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": [
        "warn",
        {
            "trailingComma": "es5",
            "tabWidth": 4,
            "semi": true,
            "singleQuote": true,
            "jsxSingleQuote": false
        }
    ]

    }
  }